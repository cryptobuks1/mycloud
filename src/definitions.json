          "KeyType": "HASH"
        }
      ],
      "ProvisionedThroughput": {
        "Fn::If": [
          "DoProvisionDynamoDBScaling",
          {
            "ReadCapacityUnits": 5,
            "WriteCapacityUnits": 5
          },
          {
            "Ref": "AWS::NoValue"
          }
        ]
      },
      "PointInTimeRecoverySpecification": {
        "PointInTimeRecoveryEnabled": {
          "Ref": "PointInTimeRecoveryEnabledEventsTable"
        }
      },
      "SSESpecification": {
        "Fn::If": [
          "DoEncrypt",
          {
            "SSEEnabled": true,
            "KMSMasterKeyID": {
              "Ref": "EncryptionKey"
            }
          },
          {
            "Ref": "AWS::NoValue"
          }
        ]
      },
      "BillingMode": {
        "Fn::If": [
          "DoProvisionDynamoDBScaling",
          "PROVISIONED",
          "PAY_PER_REQUEST"
        ]
      }
    }
  },
